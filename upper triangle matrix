#include<stdio.h>
int main(){
 int i,j,r,c;
 printf("Enter the row and column of the matrix");
 scanf("%d%d",&r,&c);
 int mat[r][c];
   if(r!=c)
   {
       return 0;
   }
 for(i=0;i<r;i++)
 {
   for(j=0;j<c;j++)
   scanf("%d",&mat[i][j]);
 }
  
  for(i=0;i<r;i++)
  {for(j=0;j<c;j++)
  {if(i<=j)
  {printf("%d ",mat[i][j]);
  }
  else
  printf("  ");
  }printf("\n");
  }
  return 0;
}

